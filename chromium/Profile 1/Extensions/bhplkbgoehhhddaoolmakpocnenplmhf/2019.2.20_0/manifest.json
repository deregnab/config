{
   "author": "Alexander Choporov (CoolCmd)",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "common.js", "content.js" ],
      "matches": [ "https://www.twitch.tv/*", "https://m.twitch.tv/*" ],
      "run_at": "document_start"
   } ],
   "default_locale": "en",
   "description": "__MSG_M0001__",
   "developer": {
      "name": "Alexander Choporov (CoolCmd)"
   },
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "49",
   "name": "Alternate Player for Twitch.tv",
   "optional_permissions": [ "contentSettings" ],
   "permissions": [ "storage", "cookies", "webRequest", "webRequestBlocking", "management", "system.memory", "system.cpu", "http://*/*", "https://*/*" ],
   "short_name": "Alternate Player",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2019.2.20",
   "web_accessible_resources": [ "player.html", "content.css" ]
}
